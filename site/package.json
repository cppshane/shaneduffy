{
  "name": "shaneduffy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run shaneduffy:serve-ssr",
    "serve:ssr": "node dist/shaneduffy/server/main.js",
    "build:ssr": "ng build --prod && ng run shaneduffy:server:production",
    "prerender": "ng run shaneduffy:prerender --routes-file routes.txt",
    "docker-dev:ssr": "docker-compose -f docker-compose.dev.yml up --build",
    "docker-prod:ssr": "docker-compose -f docker-compose.prod.yml up --build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/platform-server": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@nguniversal/express-engine": "^13.1.0",
    "@nguniversal/builders": "^13.1.0",
    "express": "^4.15.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
